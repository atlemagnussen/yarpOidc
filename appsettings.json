{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Server": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxyArr": {
    "Routes": {
      "routeRadarr": {
        "AuthorizationPolicy": "Protection",
        "ClusterId": "Radarr",
        "Match": {
          "Path": "{**catch-all}",
           "Hosts" : ["radarr.logout.work"]
        }
      },
      "routeSonarr": {
        "AuthorizationPolicy": "Protection",
        "ClusterId": "Sonarr",
        "Match": {
          "Path": "{**catch-all}",
           "Hosts" : ["sonarr.logout.work"]
        }
      },
      "route1": {
        "AuthorizationPolicy": "Protection",
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "Radarr": {
        "Destinations": {
          "radarr": {
            "Address": "http://srv.logout.work:7878/"
          }
        }
      },
      "Sonarr": {
        "Destinations": {
          "sonarr": {
            "Address": "http://srv.logout.work:8989/"
          }
        }
      },
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  }
}