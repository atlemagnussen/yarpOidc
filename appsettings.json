{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Server": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxyRadarr": {
    "Routes": {
      "route1": {
        "AuthorizationPolicy": "Protection",
        "ClusterId": "Radarr",
        "Match": {
          "Path": "{**catch-all}",
           "Hosts" : ["radarr.logout.work"]
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "radarr": {
            "Address": "http://srv.logout.work:7878/"
          }
        }
      }
    }
  },
  "ReverseProxySonarr": {
    "Routes": {
      "route1": {
        "AuthorizationPolicy": "Protection",
        "ClusterId": "Radarr",
        "Match": {
          "Path": "{**catch-all}",
           "Hosts" : ["sonarr.logout.work"]
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "radarr": {
            "Address": "http://srv.logout.work:8989/"
          }
        }
      }
    }
  }
}